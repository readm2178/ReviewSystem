                    android:textColor="@color/black_color"
                    android:textSize="16sp" />
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/cardView4"
                android:layout_width="149dp"
                android:layout_height="99dp"
                android:layout_marginStart="8dp"
